<?xml version="1.0" encoding="UTF-8"?>
<messageStore class="org.apache.synapse.message.store.impl.jdbc.JDBCMessageStore" name="SampleJDBCMessageStore" xmlns="http://ws.apache.org/ns/synapse">
    <parameter name="store.jdbc.driver">driver_name_here</parameter>
    <parameter name="store.producer.guaranteed.delivery.enable">false</parameter>
    <parameter name="store.jdbc.username">username_here</parameter>
